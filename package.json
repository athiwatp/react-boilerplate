{
  "name": "react-boilerplate",
  "version": "0.6.1",
  "description": "The React-Boilerplate is boilerplate app based on official create-react-app cli of Facebook",
  "keywords": [
    "react",
    "create-react-app",
    "webpack",
    "material",
    "sementic-ui",
    "less",
    "sass",
    "scss"
  ],
  "author": "Rhio Kim <rhio.kim@gmail.com>",
  "license": "ISC",
  "private": true,
  "devDependencies": {
    "autoprefixer": "6.7.2",
    "babel-core": "6.22.1",
    "babel-eslint": "^7.2.0",
    "babel-jest": "18.0.0",
    "babel-loader": "6.2.10",
    "babel-preset-react-app": "^2.1.1",
    "babel-runtime": "^6.20.0",
    "case-sensitive-paths-webpack-plugin": "1.1.4",
    "chalk": "1.1.3",
    "commitizen": "^2.9.6",
    "connect-history-api-fallback": "1.3.0",
    "conventional-changelog-cli": "^1.2.0",
    "cross-spawn": "4.0.2",
    "css-loader": "0.26.1",
    "cz-conventional-changelog": "^2.0.0",
    "detect-port": "1.0.1",
    "dotenv": "2.0.0",
    "eslint": "3.16.1",
    "eslint-config-react-app": "^0.6.2",
    "eslint-formatter-pretty": "^1.1.0",
    "eslint-loader": "1.6.0",
    "eslint-plugin-flowtype": "^2.30.4",
    "eslint-plugin-import": "2.0.1",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.4.1",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.10.0",
    "filesize": "3.3.0",
    "find-cache-dir": "0.1.1",
    "flow-bin": "^0.42.0",
    "fs-extra": "0.30.0",
    "git-repo-info": "^1.3.1",
    "gzip-size": "3.0.0",
    "html-webpack-plugin": "2.24.0",
    "http-proxy-middleware": "0.17.3",
    "husky": "^0.13.2",
    "jest": "18.1.0",
    "json-loader": "0.5.4",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "node-sass": "^4.5.0",
    "object-assign": "4.1.1",
    "path-exists": "2.1.0",
    "postcss-loader": "1.2.2",
    "promise": "7.1.1",
    "react-dev-utils": "^0.5.2",
    "recursive-readdir": "^2.1.1",
    "redux-devtools": "^3.3.2",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.2.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.2",
    "strip-ansi": "3.0.1",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "validate-commit-msg": "^2.8.2",
    "webpack": "1.14.0",
    "webpack-dev-server": "1.16.2",
    "webpack-manifest-plugin": "1.1.0",
    "whatwg-fetch": "2.0.2"
  },
  "dependencies": {
    "axios": "^0.15.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-helmet": "^4.0.0",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "redux-logger": "^2.8.1",
    "redux-thunk": "^2.2.0"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "lint": "eslint --format=node_modules/eslint-formatter-pretty 'src/**/*.@(js|jsx)'",
    "flow": "flow",
    "lint:msg": "validate-commit-msg",
    "deploy": "npm run build && npm run docker:build",
    "commit": "git cz",
    "precommit": "npm run lint && flow",
    "commitmsg": "npm run lint:msg",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 1",
    "docker:build": "docker build --tag react-app -f ./config/docker/Dockerfile . && npm run docker:images",
    "docker:images": "docker images | grep react-app",
    "docker:run": "docker run -dit -p 8081:80 --name react-app react-app:latest",
    "docker:rm": "docker rm -f react-app",
    "docker": "npm run docker:rm && npm run docker:build && npm run docker:run"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules|scripts)[/\\\\]"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "plugin:flowtype/recommended",
      "react-app"
    ],
    "plugins": [
      "flowtype"
    ],
    "settings": {
      "flowtype": {
        "onlyFilesWithFlowAnnotation": false
      }
    }
  },
  "config": {
    "validate-commit-msg": {
      "preset": "angular",
      "warnOnFail": false,
      "maxSubjectLength": 100
    },
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
